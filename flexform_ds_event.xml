 <T3DataStructure>
  <meta>
        <langDisable>1</langDisable>
  </meta>
  <sheets>
   <sDEF>
    <ROOT>
     <TCEforms>
      <sheetTitle>LLL:EXT:myevents/locallang_db.xml:tt_content.pi_flexform.sheet_general</sheetTitle>
     </TCEforms>
	  <type>array</type>
       <el>
			  <location>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_locations</label>
                                <config>
										<type>select</type>
										<foreign_table>tx_myevents_locations</foreign_table>
										<size>6</size>
										<maxitems>2</maxitems>
										<selectedlistStyle>width:350px;</selectedlistStyle>
                                </config>
                        </TCEforms>
              </location>  
			  <starttime>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.starttime</label>
                                <config>
                                        <type>input</type>
                                        <size>30</size>
										<eval>datetime</eval>
                                </config>
                        </TCEforms>
              </starttime> 
			  <endtime>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.endtime</label>
                                <config>
                                        <type>input</type>
                                        <size>30</size>
										<eval>datetime</eval>
                                </config>
                        </TCEforms>
              </endtime>
			  <teaser>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.teaser</label>
                                <config>
                                        <type>text</type>
                                        <size>30</size>
										<cols>40</cols>
										<rows>5</rows>
                                </config>
								<defaultExtras>richtext[]</defaultExtras>
                        </TCEforms>
              </teaser>  
            </el>
     </ROOT>
    </sDEF>
	<options>
		<ROOT>
		 	<TCEforms>
      			<sheetTitle>LLL:EXT:myevents/locallang_db.xml:tt_content.pi_flexform.sheet_options</sheetTitle>
     		</TCEforms>
	  		<type>array</type>
       		<el>
       			<category>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_categories</label>
                                <config>
										<type>select</type>
										<foreign_table>tx_myevents_categories</foreign_table>
										<size>6</size>
										<selectedlistStyle>width:350px;</selectedlistStyle>
										<maxitems>2</maxitems>
                                </config>
                        </TCEforms>
              	</category>
       			<discipline>
                        <TCEforms>
                        	<label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.discipline</label>
                                <config>
                                	<type>select</type>
									<size>6</size>
									<maxitems>6</maxitems>
					                <selectedlistStyle>width:350px</selectedlistStyle>
									<items type="array">
									    <numIndex index="0" type="array">
									    	<numIndex index="0">Kommunikationsdesign</numIndex>
									    	<numIndex index="1">Kommunikationsdesign</numIndex>
									    </numIndex>
									    <numIndex index="1" type="array">
										    <numIndex index="0">Modedesign</numIndex>
										    <numIndex index="1">Modedesign</numIndex>
									    </numIndex>
									    <numIndex index="2" type="array">
										    <numIndex index="0">Intermediales Design</numIndex>
										    <numIndex index="1">Intermediales Design</numIndex>
									    </numIndex>
									    <numIndex index="3" type="array">
										    <numIndex index="0">Architektur</numIndex>
										    <numIndex index="1">Architektur</numIndex>
								   	    </numIndex>
										 <numIndex index="4" type="array">
										    <numIndex index="0">Innenarchitektur</numIndex>
										    <numIndex index="1">Innenarchitektur</numIndex>
								   	    </numIndex>
										 <numIndex index="5" type="array">
										    <numIndex index="0">Edelstein und Schmuckdesign</numIndex>
										    <numIndex index="1">Edelstein und Schmuckdesign</numIndex>
								   	    </numIndex>
								    </items>
									
                                </config>
                        </TCEforms>
              </discipline>
       		</el>
		</ROOT>
    </options>
	<registration>
		<ROOT>
		 	<TCEforms>
      			<sheetTitle>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.registration</sheetTitle>
     		</TCEforms>
	  		<type>array</type>
       		<el>
       			<enablereg>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.enablereg</label>
                                <config>
                                        <type>check</type>
										<eval>datetime</eval>
										<default>0</default>
                                </config>
                        </TCEforms>
              	</enablereg>
       			<regstart>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.regstart</label>
                                <config>
                                        <type>input</type>
                                        <size>30</size>
										<eval>datetime</eval>
                                </config>
                        </TCEforms>
              	</regstart>
				<fee>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.fee</label>
                                <config>
                                        <type>input</type>
                                        <size>30</size>
                                </config>
                        </TCEforms>
              	</fee>
				<maxattendees>
                        <TCEforms>
                                <label>LLL:EXT:myevents/locallang_db.xml:tx_myevents_event.maxattendees</label>
                                <config>
                                        <type>input</type>
                                        <size>30</size>
										<eval>int</eval>
                                </config>
                        </TCEforms>
              	</maxattendees>
       		</el>
		</ROOT>
    </registration>
   </sheets>
 </T3DataStructure>
 
		
		